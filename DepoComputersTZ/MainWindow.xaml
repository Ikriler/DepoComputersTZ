<Window x:Class="DepoComputersTZ.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DepoComputersTZ"
        mc:Ignorable="d"
        Title="DepoComputersTZ" Height="450" Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="GoOrganizationsWindowButton" Grid.Column="1" Grid.Row="1" FontFamily="Arial" FontSize="24" Background="#FF269DEA" Click="GoOrganizationsWindowButton_Click">Организации</Button>
        <Button x:Name="GoWorkersWindowButton" Grid.Column="1" Grid.Row="3" FontFamily="Arial" FontSize="24" Background="#FF269DEA" Click="GoWorkersWindowButton_Click">Сотрудники</Button>
    </Grid>
</Window>
